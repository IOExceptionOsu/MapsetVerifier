<Window x:Class="MapsetVerifier.OpenDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MapsetVerifier"
        Title="Open Map" Height="400" Width="300">
    <DockPanel>
        <TextBlock Text="OSU! Folder: " Grid.Column="0" Grid.Row="0" />
        <TextBox HorizontalAlignment="Stretch" Text="" Grid.Column="1" />
        <ListBox ItemsSource="{Binding MapsetList, Mode=OneWay}" />
        <ListBox ItemsSource="{Binding Osuasdf, Mode=OneWay}" />
    </DockPanel>
</Window>